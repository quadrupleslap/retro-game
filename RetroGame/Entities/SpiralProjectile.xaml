<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RetroGame"
             xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="SpiralProjectile"
             mc:Ignorable="d">
    <ed:RegularPolygon
        x:Name="Square"
        Fill="White"
        Height="32" Width="32"
        InnerRadius="0.47211" PointCount="5"
        Stretch="Fill" StrokeThickness="1"
        RenderTransformOrigin="0.5,0.5">
        <ed:RegularPolygon.RenderTransform>
            <RotateTransform x:Name="Spinning" Angle="0"/>
        </ed:RegularPolygon.RenderTransform>
    </ed:RegularPolygon>
</UserControl>
